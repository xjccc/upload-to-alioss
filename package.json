{
  "name": "upload-to-alioss",
  "displayName": "upload-to-alioss",
  "description": "it is a extension for users upload to ali oss",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:upload-to-alioss.helloWorld",
    "onCommand:upload-to-alioss.uploadFile",
    "onCommand:upload-to-alioss.uploadFolder"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "upload-to-alioss.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "upload-to-alioss.uploadFile",
        "title": "上传文件到阿里云OSS"
      },
      {
        "command": "upload-to-alioss.uploadFolder",
        "title": "上传文件夹到阿里云OSS"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "upload-to-alioss.uploadFolder",
          "when": "explorerResourceIsFolder == true",
          "group": "navigation@2"
        },
        {
          "command": "upload-to-alioss.uploadFile",
          "when": "explorerResourceIsFolder != true",
          "group": "navigation@2"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.11",
    "@types/dotenv": "^8.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "ali-oss": "^6.23.0",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "proxy-agent": "^6.5.0"
  }
}
